{
  "connector": "local",
  "extends": [
    "development",
    "accessibility",
    "progressive-web-apps"
  ],
  "formatters": [
    "html",
    "summary",
    "codeframe"
  ],
  "browserslist": [
    "defaults",
    "not ie 11",
    "not and_chr <= 111",
    "not chrome <= 111",
    "not edge <= 111",
    "not firefox <= 111",
    "not safari <= 16.3",
    "not samsung <= 20",
    "not ios_saf <= 14.8",
    "not and_ff <= 110",
    "not and_ff <= 110",
    "not ios_saf <= 16.3",
    "not opera <= 95",
    "not ios_saf <= 16.3"
  ],
  "hints": {
    "axe": "error",
    "babel-config/is-valid": "error",
    "disown-opener": "error",
    "highest-available-document-mode": "error",
    "manifest-exists": "off",
    "meta-charset-utf-8": "error",
    "meta-viewport": "error",
    "compat-api/css": [
      "off",
      {
        "ignore": [
          "overflow-anchor",
          "-webkit-overflow-scrolling"
        ]
      }
    ],
    "content-type": [ "error", {
        ".*\\.js": "application/javascript; charset=utf-8"
      } ]
  },
  "hintsTimeout": 10000
}
