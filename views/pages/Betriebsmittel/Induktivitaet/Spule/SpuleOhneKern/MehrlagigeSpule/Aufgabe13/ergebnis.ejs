<%- include('aufgabe.ejs',{}) %>
<%- include('../../../../../../../layouts/partials/ergebnis.ejs') %>

<script type="module">

    import {drawkennzeichnung} from '/modules/drawkennzeichnung.mjs'
    import {tabulate} from '/modules/tabulate.mjs'
    import {drawboard} from '/modules/drawboard.mjs'
    import {populate} from '/modules/populate.mjs'

    import {vMehrlagigeSpule} from '/components/Betriebsmittel/Induktivitaet/Spule/SpuleOhneKern/MehrlagigeSpule/MehrlagigeSpule.mjs'

    // tabelle ergebnis
    const data=await populate( "http://localhost:8000/json/example/aufgabe13.json" )

    // Ausgabe Kennzeichnung
    drawkennzeichnung( data,"#kennzeichnung" )

    // Ausgabe Datentabelle
    tabulate( data,"#ergebnis" )

    // RÃ¼ckkopplung zum Eingebeformular
    const sel=d3.selectAll( 'tbody' ).selectAll( 'tr' )._groups[ 0 ][ 0 ].innerText.split( '\t' )
    Material.value=sel[ 0 ]
    lm.value=sel[ 1 ]
    N.value=sel[ 2 ]
    d.value=sel[ 3 ]

    // Visualisierung
    const g=drawboard()
    let vL=new vMehrlagigeSpule( data )
    g.append( () => vL.createNode() )


</script>
