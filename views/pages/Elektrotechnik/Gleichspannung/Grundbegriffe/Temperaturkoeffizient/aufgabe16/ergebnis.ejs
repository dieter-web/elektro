<%- include('aufgabe.ejs',{}) %>
<%- include('../../../../../../layouts/partials/ergebnis.ejs') %>

<script type="module">

    import {drawkennzeichnung} from '/modules/drawkennzeichnung.mjs'
    import {populate} from '/modules/populate.mjs'
    import {tabulate} from '/modules/tabulate.mjs'
    import {drawboard} from '/modules/drawboard.mjs'
    import {Material} from '/d3/lib/Material/Material.mjs'

    // tabelle ergebnis
    const data=await populate( "http://localhost:8000/json/example/aufgabe16.json" )

    const mat=await populate( "http://localhost:8000/json/Sonstiges/EigenschaftenVonMetallen.json" )

    // drawKennzeichnung( data,"#kennzeichnung" )
    tabulate( data,"#ergebnis" )

    // RÃ¼ckkopplung
    const tab=d3.selectAll( 'tbody' ).selectAll( 'tr' )._groups[ 0 ][ 0 ].innerText.split( '\t' );
    Material.value=tab[ 0 ]

    // Visualisierung
    const g=drawboard();
    let M=new Material( data )
    g.append( () => M.fdraw() )



</script>
