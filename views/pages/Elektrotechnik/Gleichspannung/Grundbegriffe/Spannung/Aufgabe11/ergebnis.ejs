<%- include('aufgabe.ejs',{}) %>
<%- include('../../../../../../layouts/partials/ergebnis.ejs') %>


<script type="module">
    import {drawkennzeichnung} from '/modules/drawkennzeichnung.mjs'
    import {tabulate} from '/modules/tabulate.mjs'
    import {drawboard} from '/modules/drawboard.mjs'
    import {populate} from '/modules/populate.mjs'
    import {vRClass} from '/d3/lib/Betriebsmittel/vRClass.mjs'

    // tabelle ergebnis
    const data=await populate( "http://localhost:8000/json/example/aufgabe11.json" )

    drawkennzeichnung( data,"#kennzeichnung" )
    tabulate( data,"#ergebnis" )

    // RÃ¼ckkopplung
    const tab=d3.selectAll( 'tbody' ).selectAll( 'tr' )._groups[ 0 ][ 0 ].innerText.split( '\t' );
    G.value=tab[ 0 ]
    I.value=tab[ 1 ]

    // Visualisierung
    const g=drawboard()
    let vG1=new vRClass( data )
    g.append( () => vG1.fdraw() )

</script>
